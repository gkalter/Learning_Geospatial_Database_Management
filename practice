DROP TABLE CUSTOMER;
CREATE TABLE CUSTOMER (
	ID SERIAL,
	NAME TEXT,
	AGE INT,
	ADDRESS VARCHAR(100),
	TELEPHONE INT
	)
ALTER TABLE CUSTOMER
ADD COLUMN RANDOM TEXT;

INSERT INTO CUSTOMER(NAME)
VALUES
	('ADAM'),
	('BILLY'),
	('ROSE');
UPDATE CUSTOMER
SET NAME = 'JOHN'
WHERE ID = 1;

SELECT * FROM CUSTOMER;